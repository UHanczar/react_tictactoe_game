{
  "extends": "airbnb",

  "parser": "babel-eslint",

  "env": {
      "browser": true,
      "node": true
  },

  "env": {
      "browser": true,
      "node": true
    },

    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],

    "rules": {
      "linebreak-style": [2, "windows"],
      "semi": [1, "always"],
      "no-debugger": 0,
      "no-console": 0,
      "new-cap": 0,
      "strict": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "eol-last": 1,
      "comma-dangle": [0, "always"],
      "jsx-quotes": ["error", "prefer-single"],
      "react/prefer-stateless-function": 0,
      "react/prefer-es6-class": 0
    }
}
